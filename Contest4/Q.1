/*

Find x and y 
Input
4
1 1
2 3
3 5
6 9
Output
-1 1
-4 9
-18 50
-4 9

*/


import java.util.*;
public class Main {
 
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t>0) {
			t--;
			long u = sc.nextLong();
			long v = sc.nextLong();
			long x = -u*u;
			long y = v*v;
			System.out.println(x+" "+y);
		}
	}
 
}
